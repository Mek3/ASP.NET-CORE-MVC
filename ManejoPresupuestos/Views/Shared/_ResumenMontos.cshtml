@model ResumenMontosViewModel


<div class="contenedor-totales mb-3">
    <div>
        <div>
            <strong>Depósitos</strong>
        </div>
        <div class="activo" style="text-align:right">
            @Model.Ingresos.ToString("N")
        </div>

    </div>
    <div>
        <div>
            <strong>Retiros</strong>
        </div>
        <div class="pasivo" style="text-align:right">
            @Model.Gastos.ToString("N")
        </div>

    </div>

    <div>
        <div>
            <strong>Total</strong>
        </div>
        <div style="text-align:right">
            @Model.Total.ToString("N")
        </div>

    </div>

</div>